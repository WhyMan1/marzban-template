{
  "__Credits__": {
    "repository": "https://github.com/WhyMan1/marzban-template/tree/master/v2ray",
    "telegram": "https://t.me/TemplatesMarzban",
    "donate": "TRX (TRC20): TSxZbLUMgzehF9bSo2GLb36sP2wg34ywev"
  },
  "log": {
    "loglevel": "warning",
    "dnsLog": false,
    "access": "none"
  },
  "dns": {
    "hosts": {
      "one.one.one.one": [
        "1.1.1.1",
        "1.0.0.1",
        "2606:4700:4700::1111",
        "2606:4700:4700::1001"
      ]
    },
    "servers": [
      "one.one.one.one"
    ],
    "queryStrategy": "UseSystem",
    "useSystemHosts": true
  },
  "inbounds": [
    {
      "tag": "socks-in",
      "listen": "127.0.0.1",
      "port": 10808,
      "protocol": "mixed",
      "settings": {
        "udp": true
      },
      "sniffing": {
        "enabled": false
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "tag": "dns-out",
      "protocol": "dns"
    }
  ],
  "routing": {
    "rules": [
      {
        "port": 53,
        "inboundTag": [
          "socks-in"
        ],
        "outboundTag": "dns-out"
      },
      {
        "ip": [
          "geoip:ir",
          "geoip:private"
        ],
        "outboundTag": "direct"
      }
    ]
  }
}